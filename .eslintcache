Password: '',\r\n        name: '',\r\n        lastName: '',\r\n        date: '',\r\n        nickname: ''\r\n    }\r\n    const [error2, setError2] = useState(null);\r\n    const classes = useStyles();\r\n    const [error, setError] = useState(null);\r\n    const [formState, setFormState] = useState(state);\r\n\r\n    const handleChange = event => {\r\n        setFormState({\r\n            ...formState,\r\n            [event.target.name]: event.target.value,\r\n        })\r\n    }\r\n\r\n    const onSubmit = async data => {\r\n        setError2(null)\r\n        setError(null)\r\n\r\n        registro();\r\n        /* console.log(formState); */\r\n\r\n    };\r\n\r\n\r\n\r\n\r\n\r\n\r\n    const registro = React.useCallback(async () => {\r\n        try {\r\n\r\n            const d = new Date();\r\n            const c = new Date(selectedDate);\r\n            const a = c.getFullYear();\r\n            const n = d.getFullYear();\r\n\r\n            const edad = n - a;\r\n            if (edad < 18) {\r\n                setError2(\"Tienes que ser mayor de 18\")\r\n                return;\r\n            }\r\n            /* console.log(edad)\r\n            \r\n            console.log(n) */\r\n\r\n            const res = await auth.createUserWithEmailAndPassword(formState.email, formState.password)\r\n            await db.collection('users').doc(res.user.uid).set({\r\n                email: res.user.email,\r\n                name: formState.name,\r\n                lastName: formState.lastName,\r\n                date: selectedDate,\r\n                country: formState.country,\r\n            });\r\n\r\n\r\n\r\n        } catch (error) {\r\n            /* console.log(error) */\r\n            // setError(error.message)\r\n            if (error.code === 'auth/email-already-in-use') {\r\n                setError('Email ya registrado')\r\n                return;\r\n            }\r\n            if (error.code === 'auth/invalid-email') {\r\n                setError('Email no válido')\r\n                return;\r\n            }\r\n        }\r\n\r\n\r\n\r\n    })\r\n\r\n\r\n\r\n    return (\r\n        <div className=\"formulario\">\r\n            <Grid\r\n                container\r\n                direction=\"row\"\r\n                justify=\"center\"\r\n                alignItems=\"center\"\r\n            >  <div className=\"log-form\" >\r\n                    <ThemeProvider theme={theme}>\r\n\r\n                        <form className={classes.root} onSubmit={handleSubmit(onSubmit)} >\r\n                            {error && <div className=\"alert\"><p>{error}</p></div>}\r\n                            {errors.email && <div className=\"alert\"><p>{errors.email.message}</p></div>}\r\n\r\n                            <TextField name=\"email\" underline={false} className={classes.sortFormLabel} id=\"standard-required\" label=\"email\" defaultValue=\"example@gmail.com\"\r\n                                onChange={handleChange}\r\n                                InputProps={{\r\n                                    className: classes.input\r\n                                }}\r\n\r\n                                ref={register({\r\n                                    name: 'email',\r\n                                    required: \"Parametro requerido.\",\r\n                                    pattern: {\r\n                                        value: /^[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,}$/i,\r\n                                        message: \"Caracteres no validos.\"\r\n                                    },\r\n\r\n                                })}\r\n\r\n                            />\r\n                            {errors.password && <div className=\"alert\"><p>{errors.password.message}</p></div>}\r\n                            <TextField name=\"password\"\r\n                                onChange={handleChange}\r\n                                id=\"standard-password-input\"\r\n                                label=\"password\"\r\n 